{
    "version": 2,
    "builds": [
      {
        "src": "./index.ts",
        "use": "@vercel/node"
      }
    ],
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./index.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "PATCH",
          "DELETE",
          "OPTIONS"
        ]
      }
    ],
  
    "env": {
      "NODE_ENV": "development",
      "PORT": "3000"
    }
  }


